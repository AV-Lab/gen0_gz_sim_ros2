            <geometry>
            <!--cylinder length="${tyre_length}" radius="${tyre_radius}"/-->
            <mesh scale="1.2 1 1" filename="package://gen0_main/meshes/ezmile_gen0_wheel.obj"/>
            </geometry>
        </visual>

        <inertial>
